    <Column ColName='BeginDate' Dis='开始时间'   IsNotNull='true'  IsNotNullMessage='请输入开始时间'/>
    <Column ColName='EndDate' Dis='结束时间'   IsNotNull='true'  IsNotNullMessage='请输入结束时间'/>
