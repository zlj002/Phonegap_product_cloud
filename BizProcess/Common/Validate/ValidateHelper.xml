    <Column ColName='RewardName' Dis='奖励名称'   IsNotNull='true'  IsNotNullMessage='请输入奖励名称'/>
    <Column ColName='GetDate' Dis='颁奖日期'   IsNotNull='true'  IsNotNullMessage='请输入颁奖日期'/>
