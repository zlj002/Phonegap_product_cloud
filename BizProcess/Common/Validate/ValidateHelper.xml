    <Column ColName='FruitLevel' Dis='级别'   IsNotNull='true'  IsNotNullMessage='请输入级别'/>
    <Column ColName='GetDate' Dis='时间'   IsNotNull='true'  IsNotNullMessage='请输入时间'/>
  </Basic_TeacherInfo_Fruit>
